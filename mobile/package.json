{
  "name": "@arcadecity/neon-mobile",
  "version": "0.0.1",
  "description": "Neon mobile app",
  "private": true,
  "main": "crna-entry.js",
  "scripts": {
    "prestart": "node link-workspaces.js",
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "jest"
  },
  "dependencies": {
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.14",
    "@types/react-test-renderer": "^16.0.1",
    "crna-make-symlinks-for-yarn-workspaces": "^1.0.1",
    "jest-expo": "~27.0.0",
    "metro-bundler-config-yarn-workspaces": "^1.0.3",
    "react-art": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-native-web": "^0.8.9",
    "react-test-renderer": "16.3.1",
    "styled-components": "^3.4.0",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.1",
    "typescript": "^2.9.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
