{
  "name": "@arcadecity/neon-desktop",
  "version": "0.0.1",
  "description": "Neon desktop app",
  "author": "Arcade City, Inc.",
  "license": "GPL-3.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "yarn lint && react-scripts-ts start",
    "build": "yarn lint && react-scripts-ts build",
    "compile:test": "tsc --p ./tests/tsconfig.json && cp ./assets/rpc.proto ./tests/dist/public/",
    "test": "yarn compile:test && ../node_modules/.bin/mocha ./tests/dist/ --opts ./tests/mocha.opts",
    "lint": "tslint --project . --fix --format verbose",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "postinstall": "yarn load-grpc-binary",
    "load-grpc-binary": "npm install grpc --runtime=electron --target=2.0.8 && rm package-lock.json",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "blockstack": "^18.0.4",
    "cors": "^2.8.4",
    "electron-is-dev": "^0.3.0",
    "express": "^4.16.3",
    "grpc": "^1.15.1",
    "mobx": "^4.1.0",
    "mobx-react": "^5.2.8",
    "mobx-state-tree": "^2.0.2",
    "node-localstorage": "^1.3.1",
    "react": "^16.5.1",
    "react-art": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-native": "^0.55",
    "react-native-web": "^0.9.0",
    "react-scripts-ts": "2.17.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^2.0.1",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-native": "^0.56.23",
    "awesome-typescript-loader": "^4",
    "babel-core": "^6.23.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.8.10",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^4.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.23.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.1",
    "cypress": "^3.1.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.4",
    "electron-log": "^2.2.17",
    "electron-rebuild": "^1.8.2",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "postinstall-prepare": "^1.0.1",
    "react-app-rewire-yarn-workspaces": "^1.0.3",
    "react-app-rewired": "^1.6.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "reactotron-mst": "^2.1.0",
    "reactotron-react-js": "https://github.com/ArcadeCity/reactotron-react-js/tarball/master",
    "sinon": "^6.2.0",
    "stream": "^0.0.2",
    "ts-loader": "^3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "2.9.2",
    "unexpected": "^10.39.0",
    "unexpected-sinon": "^10.10.1",
    "wait-on": "^3.0.1"
  },
  "build": {
    "appId": "arcade.city.neon-desktop",
    "productName": "Neon",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
